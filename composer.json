{
	"name": "jweiland/service-bw2",
	"description": "With this extension you can access service BW interface",
	"license": "GPL-2.0-or-later",
	"type": "typo3-cms-extension",
	"keywords": [
		"typo3",
		"TYPO3 CMS",
		"service",
		"bw"
	],
	"authors": [
		{
			"name": "Stefan Froemken",
			"email": "projects@jweiland.net",
			"role": "Developer"
		},
		{
			"name": "Hoja Mustaffa Abdul Latheef",
			"email": "projects@jweiland.net",
			"role": "Senior Developer"
		}
	],
	"homepage": "https://jweiland.net",
	"support": {
		"email": "projects@jweiland.net",
		"issues": "https://github.com/jweiland-net/service_bw2/issues",
		"source": "https://github.com/jweiland-net/service_bw2"
	},
	"require": {
		"jweiland/maps2": ">=12.0.0",
		"typo3/cms-core": "^13.4.0",
		"typo3/cms-scheduler": "^13.4.0"
	},
	"require-dev": {
		"ergebnis/composer-normalize": "^2.44",
		"roave/security-advisories": "dev-latest",
		"sjbr/static-info-tables": "^13.4",
		"typo3/coding-standards": "^0.8",
		"typo3/testing-framework": "^9.0.1"
	},
	"replace": {
		"typo3-ter/service_bw2": "self.version"
	},
	"autoload": {
		"psr-4": {
			"JWeiland\\ServiceBw2\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"JWeiland\\ServiceBw2\\Tests\\": "Tests"
		}
	},
	"config": {
		"allow-plugins": {
			"ergebnis/composer-normalize": true,
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		},
		"bin-dir": ".Build/bin",
		"sort-packages": true,
		"vendor-dir": ".Build/vendor"
	},
	"extra": {
		"typo3/cms": {
			"app-dir": ".Build",
			"extension-key": "service_bw2",
			"web-dir": ".Build/public"
		}
	}
}
